<div class= title><h1>Your Tasks Today</h1></div>


<div class= container>
<ul>
  <% @tasks.each do |task| %>
    <li><%= task.title %></li>
    <%= link_to "See Details", task_path(task) %>
    <%= link_to "Edit", edit_task_path(task)%>
    <%= link_to "Delete", delete_task_path(task), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} %>
  <% end %>
</ul>
</div>
<p><%= link_to "New Task",  new_task_path %></p>
